  "keywords": [
    "whois",
    "domain",
    "lookup",
    "dns",
    "rdap"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinanartun/whois2.git"
  },
  "bugs": {
    "url": "https://github.com/sinanartun/whois2/issues"
  },
  "homepage": "https://github.com/sinanartun/whois2#readme",
  "engines": {
    "node": ">=14.16.0"
  }
}
