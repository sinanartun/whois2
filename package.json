{
  "name": "@sinanartun/whois2",
  "version": "1.1.0",
  "description": "A simple WHOIS lookup library for Node.js",
  "license": "MIT",
  "author": "Sinan Artun",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "index.js",
    "whois-regex.js",
    "dist",
    "README.md"
  ],
